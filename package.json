{
  "name": "ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy-config.json",
    "build": "npm run build:demo",
    "build:demo": "node build-utils/replace.build.js --target demo && ng build --prod --environment demo && node build-utils/generate-index-files.js",
    "build:prod": "node build-utils/replace.build.js --target prod && ng build --prod --environment prod && node build-utils/generate-index-files.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sass-lint": "sass-lint -c sass-lint.yml -v -q",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json",
    "precommit": "npm run lint && ng build --prod"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "parser": "typescript",
    "printWidth": 120,
    "singleQuote": true
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.3",
    "@angular/common": "^5.1.3",
    "@angular/compiler": "^5.1.3",
